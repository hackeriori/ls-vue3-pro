{
  "name": "ls-vue3-pro",
  "version": "1.3.2",
  "description": "基于vue3的一些组件及可复用代码块,用于工作",
  "main": "dist/lsVue3Pro.umd.js",
  "module": "dist/lsVue3Pro.es.js",
  "repository": {
    "type": "git",
    "url": "https://gitee.com/song_qing_xing/ls-vue3-pro"
  },
  "homepage": "https://song_qing_xing.gitee.io/ls-vue3-pro/",
  "files": [
    "dist",
    "types"
  ],
  "typings": "types/index.d.ts",
  "web-types": "types/web-types.json",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit --allowJs && vite build && esbuild dist/lsVue3Pro.es.js --minify --format=esm --allow-overwrite --outfile=dist/lsVue3Pro.es.js",
    "buildHtml": "vite build --config vite.config.html.ts",
    "preview": "vite preview"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "element-plus": "^2.9.7",
    "ls-base-lib": "^2.0.3"
  },
  "devDependencies": {
    "@codemirror/lang-javascript": "^6.2.3",
    "@codemirror/lang-vue": "^0.1.3",
    "@esbuild/win32-x64": "^0.18.13",
    "@types/node": "^18.14.1",
    "@vitejs/plugin-vue": "^5.2.3",
    "codemirror": "^6.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.3",
    "unplugin-auto-import": "^19.1.2",
    "unplugin-vue-components": "^28.5.0",
    "vite": "^6.3.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue-tsc": "^2.2.8"
  }
}
